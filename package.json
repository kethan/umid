{
	"name": "umid",
	"version": "4.0.0",
	"description": "A generic and lightweight express like middleware with zero dependencies",
	"main": "./dist/index.js",
	"type": "module",
	"module": "./dist/index.es.js",
	"unpkg": "./dist/index.min.js",
	"umd:main": "./dist/index.umd.js",
	"exports": {
		".": {
			"browser": "./dist/index.es.js",
			"umd": "./dist/index.umd.js",
			"require": "./dist/index.js",
			"import": "./dist/index.es.js",
			"default": "./dist/index.es.js"
		}
	},
	"scripts": {
		"build": "rollup --config rollup.config.js",
		"test": "vitest"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"middleware",
		"express",
		"use",
		"pipes",
		"pipe"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kethan/umid.git"
	},
	"author": "Kethan Surana",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kethan/umid/issues"
	},
	"homepage": "https://github.com/kethan/umid#readme",
	"devDependencies": {
		"@rollup/plugin-terser": "^0.4.4",
		"rollup": "^4.20.0",
		"vitest": "^2.0.5"
	}
}
